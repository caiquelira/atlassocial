<script type="text/javascript">

	var responseData = <%-JSON.stringify(responseData)%>;
	function WindowCloseItself() {
		try {
			window.opener.loginCallBack(responseData);
		}
		catch (err) {
			window.opener.loginCallBack(null, err);
		}

		window.close();
	}
	
	window.onpaint = WindowCloseItself();
</script>