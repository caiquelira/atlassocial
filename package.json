{
  "name": "atlassocial",
  "version": "1.0.0",
  "description": "Atlas Social",
  "main": "app.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "clean": "rm -rf ./public/build",
    "build:dev": "webpack --progress --colors --config webpack.config.dev.js ",
    "build:prod": "NODE_ENV=production webpack --progress --colors --config webpack.config.prod.js",
    "build": "npm run clean && npm run build:dev",
    "postinstall": "npm run clean && npm run build:prod",
    "react": "rm -rf ./public/build && set NODE_ENV=react && node app.js",
    "start": "node app.js"
  },
